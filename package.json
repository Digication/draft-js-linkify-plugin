{
  "name": "draft-js-linkify-plugin",
  "version": "2.0.0-beta9",
  "description": "Linkify Plugin for DraftJS",
  "author": {
    "name": "Nik Graf",
    "email": "nik@nikgraf.com",
    "url": "http://www.nikgraf.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/draft-js-plugins/draft-js-plugins.git"
  },
  "main": "lib/index.js",
  "keywords": [
    "editor",
    "wysiwyg",
    "draft",
    "react",
    "ux",
    "components",
    "widget",
    "react-component"
  ],
  "scripts": {
    "clean": "../node_modules/.bin/rimraf lib",
    "build": "npm run clean && npm run build:js && npm run build:css",
    "build:js": "WEBPACK_CONFIG=$(pwd)/webpack.config.js BABEL_DISABLE_CACHE=1 BABEL_ENV=production NODE_ENV=production babel --out-dir='lib' --ignore='__test__/*' src",
    "build:css": "node concatCssFiles $(pwd) && ../node_modules/.bin/rimraf lib-css",
    "prepublish": "npm run build"
  },
  "license": "MIT",
  "dependencies": {
    "babel-cli": "^6.18.0",
    "babel-plugin-webpack-loaders": "^0.8.0",
    "babel-preset-stage-0": "^6.16.0",
    "decorate-component-with-props": "^1.0.2",
    "draft-js": ">=0.9.1",
    "extract-text-webpack-plugin": "^1.0.1",
    "immutable": "~3.7.4",
    "linkify-it": "2.0.0",
    "react": "^15.4.2",
    "tlds": "1.159.0",
    "union-class-names": "^1.0.0"
  },
  "peerDependencies": {
    "react": "^15.0.0",
    "react-dom": "^15.0.0"
  }
}
